<%= form_for(@user) do |f| %>
    <%= f.label("Name")%>
    <%= f.text_field :name %> <br>

    <%= f.label("BirthDate") %>
    <%= f.date_select :birthDate , start_year: 1980, end_year: 2030%> <br>

    <%= f.label("Email")%>
    <%= f.text_field :email %> <br>

    <%= f.label("Password")%>
    <%= f.text_field :password %> <br>

    <%= f.submit('Register')%>
    

<% end %>